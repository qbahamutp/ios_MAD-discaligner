<?xml version='1.0' encoding='utf-8'?>
<widget id="discaligner-ios" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
	<name>MAD Disc Aligners</name>
	<description>
		Gebouwd in opdracht van MAD, gericht aan hun verkopers en distributeurs.
	</description>
	<author email="ad@trendesigne.nl" href="http://www.trendesign.nl">
		Trendesign
	</author>
	<content src="index.html" />
	<access origin="*" />
	<preference name="fullscreen" value="true" />
	<preference name="webviewbounce" value="false" />
	<preference name="DisallowOverscroll" value="true" />
	<preference name="UIWebViewBounce" value="false" />
</widget>
